public class Solution {  

    public static int pairSum(int arr[], int x) {
         int count=0;
        int temp=0;
        for(int i=0;i<arr.length;i++)
{ 
    for(int j=i+1;j<arr.length;j++)
    {
      temp=arr[i]+arr[j];
      
        if(temp==x)
        {
          count++;
    }
    }
}
        return count;
}
}
